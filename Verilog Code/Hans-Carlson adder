module HCA_adder_32bit(result,out, y, cout);
input [31:0] result,out;
output [31:0] y;
output cout;
wire [31:0] p, g, c;
assign p = result ^out ;
assign g = result&out;  
assign y[0] = p[0];
assign c[0] = g[0];
genvar i;
generate
for (i = 1; i < 32; i = i + 1) begin : HCA
assign c[i] = (p[i] & c[i-1]) | g[i];
assign y[i] = p[i] ^ c[i-1];
end
endgenerate
assign cout = c[31];
endmodule
